{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "AvgScript",
	"scopeName": "source.avgscript",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#labels"
		},
		{
			"include": "#RepExpr"
		},
		{
			"include": "#params"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#language_external"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"begin": "\\s*+(\\/\\*)",
					"captures": {
						"0": {
							"name": "punctuation.definition.comment.AvgScript"
						}
					},
					"end": "\\*\\/",
					"name": "comment.block.AvgScript"
				},
				{
					"name": "comment.singleline.AvgScript",
					"match": "(\\/\\/|\\().*"
				}
			]
		},
		"labels": {
			"name": "labels.AvgScript",
			"match": "(^\\s*)(;.*)"
		},
		"RepExpr": {
			"match": "(<)(?!.*\\/n)(?!.*\\\\n)([^=|\\||:|\\$|&|<|>|\\[|\\]]*)?(>)",
			"captures": {
				"2": {
					"name": "RepExpr.AvgScript"
				},
				"1": {
					"name": "operators.AvgScript"
				},
				"3": {
					"name": "operators.AvgScript"
				}
			}
		},
		"params": {
			"match": "(^|\\G)(\\s*)(?i)(Lang\\[.+?\\])?([@#][^=|\\||:]*)(.*)",
			"captures": {
				"3": {
					"patterns": [
						{
							"include": "#language"
						}
					]
				},
				"4": {
					"patterns": [
						{
							"include": "#keywords"
						}
					]
				},
				"5": {
					"patterns": [
						{
							"include": "#params_core"
						}
					]
				}
			}
		},
		"params_core": {
			"match": "(=|\\||:)(?!.*\\/n)(?!.*\\\\n)([^=|\\||:|\\$|&|<|>|\\[|\\]]*)(.*)",
			"name": "params.AvgScript",
			"captures": {
				"1": {
					"name": "operators.AvgScript"
				},
				"2": {
					"name": "params.AvgScript",
					"patterns": [
						{
							"include": "#numbers_dec"
						},
						{
							"include": "#numbers_hex"
						}
					]
				},
				"3": {
					"patterns": [
						{
							"include": "#params_core"
						}
					]
				}
			}
		},
		"operators": {
			"name": "operators.AvgScript",
			"match": "=|\\||:|\\/n|\\\\n|\\$|&|<|>|\\[|\\]"
		},
		"numbers_dec": {
			"name": "numbers.dec.AvgScript",
			"match": "([\\+-]?)\\b([0-9]+)(\\.[0-9]+)?\\b"
		},
		"numbers_hex": {
			"name": "numbers.hex.AvgScript",
			"match": "(#|0[xX])([0-9a-fA-F]{6})\\b"
		},
		"keywords": {
			"patterns": [
				{
					"include": "#keywords_region"
				},
				{
					"include": "#keywords_system"
				},
				{
					"include": "#keywords_values"
				},
				{
					"include": "#keywords_dialogue"
				},
				{
					"include": "#keywords_media"
				},
				{
					"include": "#keywords_effect"
				},
				{
					"include": "#keywords_preobj"
				},
				{
					"include": "#keywords_undefined"
				}
			]
		},
		"keywords_region": {
			"match": "(?i)(#begin|#end)\\b",
			"captures": {
				"1": {
					"name": "keyword.region.AvgScript"
				}
			}
		},
		"keywords_system": {
			"match": "(?i)(#Error|#NULL|#CacheClean|#UnSkipAble|#SkipAble|#SGO|#SetGlobalOffset|#TransitionSpeed|#SeparateTransitionSpeed|#ForceTransition|#Save|#debug|#debugOff|#defineRGB|#MSG|#MSGRAW|#MSGClear|#StopFF|#StopFastForward|@HideUI|@ShowUI|#UpdateUICoord|#DisableUI|#EnableUI|#FNT|#ForceNoTransition|#FNTO|#ForceNoTransitionOff|#Eval|#SideEffect|#NoSideEffect|#EOF|#WaitGeneral|#W|#wait|#FW|#ForceWait|#AutoChangePage|#TextDisplaySpeed|#jmp|#NJMP|#Call|#Ret|#FJMP|#JmpFra|#CJMP|#JmpCha|#SJMP|#SkipJmp|#FFJMP|#SkipAnchor|#SetSwitchColor|#SetSwitchHoverColor|#SetSwitchNegativeColor|#SetSwitchShader|#CreateSwitch|#Switch|#RichPresence|#UnlockAch|#AddToStat|#UnlockAppreciation|#UnlockAppreciation_Chapter|#UnlockAppreciation_Graphic|#UnlockAppreciation_Audio|#UnlockAppreciation_Video|#VNMode_Newline|#VNMode_ChangePage|#SetCapture|#CaptureSys)\\b",
			"captures": {
				"1": {
					"name": "keyword.system.AvgScript"
				}
			}
		},
		"keywords_values": {
			"match": "(?i)(#SV|#SetValue|#SVV|#SetValueValue|#SVAB|#SetValueAB|#SSS|#SetStringString|#SSAB|#SetStringAB|#VA|#ValueAdd|#VAV|#ValueAddValue|#VS|#ValueSub|#VSV|#ValueSubValue|#VM|#ValueMul|#VMV|#ValueMulValue|#VD|#ValueDiv|#VDV|#ValueDivValue|#CMP|#CMPV|#CMPValue|#CMPGeneral|#CMPVV|#CMPValueValue|#CMPAB|#CMPVAB|#CMPValueAB|#CMPSAB|#CMPStringAB|#CMPSS|#CMPStringString|#JE|#JA|#JB|#JNE)\\b",
			"captures": {
				"1": {
					"name": "keyword.values.AvgScript"
				}
			}
		},
		"keywords_dialogue": {
			"match": "(?i)(#TextAlign|#TextSpace|#DiaColor|#DiaSize|#DiaFont|#DiaShaderOon|#DiaShaderOff|#DiaOutColor|#DiaOutPixel|#DiaShadow|#NameColor|#NameSize|#NameFont|#NameShaderOn|#NameShaderOff|#NameOutColor|#NameOutPixel|#NameShadow|@dia|@DiaChange|@DiaTrans|@name|@NameChange|@NameTrans|@StashUIGraphic|@RestoreUIGraphic|@TextFadeOut)\\b",
			"captures": {
				"1": {
					"name": "keyword.dialogue.AvgScript"
				}
			}
		},
		"keywords_media": {
			"match": "(?i)(@play|@P|@Stop|@se|@BgmLoop|@BgmPreludeLoop|@BgmPre|@BgmPause|@BgmResume|@BgmFadeOut|@bgm|@BgsLoop|@BgsPause|@BgsResume|@BgsFadeOut|@bgs|@DubPlay|@dub|@DubSeque|@DubSequeOff|@DubSequePrefix|@DubChapter|@SeparateNTKChange|@NtkChange|@ntk|@PV|@PlayVideo|@ChangeVideo|@OV|@OpenVideo|@CV|@CloseVideo|@CloseVideo_Core|@VideoCodec|@VR|@VideoResume|@VP|@VideoPause|@VW|@VideoWait|@VL|@VideoLoop|@VideoFinish|@SVP|@SetVideoPos|@IgnoreStaticVideo|@VideoCache|VideoErase)\\b",
			"captures": {
				"1": {
					"name": "keyword.media.AvgScript"
				}
			}
		},
		"keywords_effect": {
			"match": "(?i)(@CreateBlur|@AddBlur|@RemoveBlur|@DestroyBlur|@BackZoomParam|@BackZoomReset|@BackZoom|@ShakeDir|@ShakeCoef|@ShakeAttenuation|@ShakeAttenuationParam|@shake|@KeepShake|@KeepShakeOff|@fade|@DestroyFade|@PFO|@PatternFadeOut|@PF|@PatternFade|@rain|@snow|@normal|@ToRain|@ToSnow|@ToNormal|@CrossFade|@KeepResolutionOff|@KeepResOff|@KeepResolution|@KeepRes|@SepiaToning|@Sepia|@ChangeSepiaStrength|@SetSepiaNoiseMotion|@ChangeSepiaNoiseMotionPeriod)\\b",
			"captures": {
				"1": {
					"name": "keyword.effect.AvgScript"
				}
			}
		},
		"keywords_preobj": {
			"match": "(?i)(@StrCenter|@StrBottom|@str|@string|@CreateStr|@CreateString|@StrS|@StrSize|@StrF|@StrFont|@StrA|@StrAlpha|@StrC|@StrColor|@MS|@MoveStr|@DestroyStr|@DestroyString|@DestroyAllStr|@DestroyAllString|@Spe|@MO|@MoveObj|@Bottom|@CG|@CGChange|@CPF|@CPatternFade|@CPFI|@CPatternFadeIn|@CPFO|@CPatternFadeOut|@CGPFI|@CGPatternFadeIn|@CGPFO|@CGPatternFadeOut|@CharPF|@CharPatternFade|@char|@character|@CC|@CharChange|@CA|@CharAlpha|@HideChar|@HideAllChar|@ShowChar|@ShowAllChar|@CharRotate|@AttachShader|@PeriodicAnimation|@CharBlur|@CharPerspective|@CharAnimation|@CharAnimationSpeed|@CharAnimationFrame|@SetAutoArrange|@CD|@CharDispose|@CAD|@CharAllDispose|@MC|@MoveChar|@Order|@Front|@Back|@Forward|@Backward)\\b",
			"captures": {
				"1": {
					"name": "keyword.preobj.AvgScript"
				}
			}
		},
		"keywords_undefined": {
			"match": "(?i)((@|#).+?)\\b",
			"captures": {
				"1": {
					"name": "keyword.undefined.AvgScript"
				}
			}
		},
		"language": {
			"match": "(?i)(Lang)(.*)",
			"captures": {
				"1": {
					"name": "language.prefix.AvgScript"
				},
				"2": {
					"patterns": [
						{
							"match": "(\\[)(.*?)(\\])",
							"captures": {
								"2": {
									"name": "language.region.AvgScript"
								},
								"1": {
									"name": "operators.AvgScript"
								},
								"3": {
									"name": "operators.AvgScript"
								}
							}
						}
					]
				}
			}
		},
		"language_external": {
			"match": "(?i)(^\\s*)(Lang\\[.+?\\])",
			"captures": {
				"2": {
					"patterns": [
						{
							"include": "#language"
						}
					]
				}
			}
		}
	}
}